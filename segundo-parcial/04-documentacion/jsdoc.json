{
    "templates": {
        "default": {
            "staticFiles": {
                "include": [
                    "./04-documentacion/static"
                ]
            }
        }
    },
    "source": {
        "include": ["./"],
        "exclude": [],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "node_modules/clean-jsdoc-theme",
        "encoding": "utf8",
        "destination": "./out/",
        "recurse": true,
        "tutorials": "path/to/tutorials"
    }
}